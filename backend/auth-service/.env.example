# Database settings (Docker PostgreSQL)
user=auth_user
password=auth_password_2025
host=localhost
port=5432
dbname=dbtetea

# JWT settings
SECRET_KEY=your_super_secret_key_that_is_long_and_secure
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Configurações Google OAuth
# Obtenha estas credenciais no Google Cloud Console:
# 1. Acesse https://console.cloud.google.com/
# 2. Crie um novo projeto ou selecione um existente
# 3. Vá para "APIs e Serviços" > "Credenciais"
# 4. Clique em "Criar credenciais" > "ID do cliente OAuth 2.0"
# 5. Configure as URIs de redirecionamento autorizadas
GOOGLE_CLIENT_ID=seu_client_id_aqui.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=seu_client_secret_aqui
GOOGLE_REDIRECT_URI=http://localhost:8003/api/v1/auth/google/callback

# Configurações CORS
ALLOWED_ORIGINS=["http://localhost:3000", "http://localhost:8000", "http://localhost:8001", "http://localhost:8002", "http://localhost:8003"]

# Debug
DEBUG_SQL=false
